<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="guitartutorandanalyser.guitartutor.BestScores">

    <ListView
        android:id="@+id/bestScoresListView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="351dp" />

    <TableLayout
        android:id="@+id/table_layout"
        android:layout_width="576dp"
        android:layout_height="343dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp">

        <TableRow
            android:id="@+id/table_row_default"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView android:id="@+id/textView1Name">

            </TextView>

            <TextView android:id="@+id/textView2Recordpoint">

            </TextView>

            <TextView android:id="@+id/textView3Recorddate">

            </TextView>

            <TextView android:id="@+id/textView4Completed">

            </TextView>


        </TableRow>


    </TableLayout>

</android.support.constraint.ConstraintLayout>
